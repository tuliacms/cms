{
    "type": "package",
    "name": "tuliacms/cms",
    "license": "MIT",
    "minimum-stability": "stable",
    "version": "0.0.1-alpha",
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-simplexml": "*",
        "ext-tokenizer": "*",
        "ext-xmlwriter": "*",
        "ext-zip": "*",
        "ausi/slug-generator": "^1.1",
        "composer/package-versions-deprecated": "1.11.99.5",
        "doctrine/annotations": "^1.13",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.10",
        "intervention/image": "^2.7",
        "jasongrimes/paginator": "^1.0",
        "ocramius/generated-hydrator": "^4.1",
        "ramsey/uuid": "^4.2",
        "requtize/assetter": "^2.0",
        "sensio/framework-extra-bundle": "^6.2",
        "swiftmailer/swiftmailer": "^6.3",
        "symfony/asset": "6.1.*",
        "symfony/console": "6.1.*",
        "symfony/dotenv": "6.1.*",
        "symfony/expression-language": "6.1.*",
        "symfony/flex": "^1.17|^2",
        "symfony/form": "6.1.*",
        "symfony/framework-bundle": "6.1.*",
        "symfony/intl": "6.1.*",
        "symfony/mailer": "6.1.*",
        "symfony/mime": "6.1.*",
        "symfony/monolog-bundle": "^3.7",
        "symfony/process": "6.1.*",
        "symfony/proxy-manager-bridge": "6.1.*",
        "symfony/runtime": "6.1.*",
        "symfony/security-bundle": "6.1.*",
        "symfony/string": "6.1.*",
        "symfony/translation": "6.1.*",
        "symfony/twig-bundle": "6.1.*",
        "symfony/validator": "6.1.*",
        "symfony/yaml": "6.1.*",
        "thunderer/shortcode": "^0.7.5",
        "twig/extra-bundle": "^3.3"
    },
    "require-dev": {
        "behat/behat": "^3.10",
        "mockery/mockery": "^1.5",
        "phpcompatibility/php-compatibility": "^9.3",
        "phpspec/phpspec": "^7.1",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-master",
        "symfony/browser-kit": "6.1.*",
        "symfony/css-selector": "6.1.*",
        "symfony/phpunit-bridge": "^6.0"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Tulia\\Bundle\\": "src/Bundle",
            "Tulia\\Component\\Datatable\\": "src/Component/datatable/src",
            "Tulia\\Component\\Image\\": "src/Component/image/src",
            "Tulia\\Component\\Importer\\": "src/Component/importer/src",
            "Tulia\\Component\\Routing\\": "src/Component/routing/src",
            "Tulia\\Component\\Shortcode\\": "src/Component/shortcode/src",
            "Tulia\\Component\\Templating\\": "src/Component/templating/src",
            "Tulia\\Component\\Theme\\": "src/Component/theme/src",
            "Tulia\\Cms\\Migrations\\": "src/Migrations",
            "Tulia\\Cms\\Shared\\": "src/Shared",
            "Tulia\\Cms\\": "src/Cms"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tulia\\Cms\\Tests\\Unit\\": "tests/unit",
            "Tulia\\Cms\\Tests\\Behat\\": "tests/behat/bootstrap",
            "Tulia\\Component\\Routing\\Tests\\Unit\\": "src/Component/routing/tests/unit"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.1.*"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
