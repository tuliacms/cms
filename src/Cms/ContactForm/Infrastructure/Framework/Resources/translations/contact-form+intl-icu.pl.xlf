            <trans-unit id="tuliacms.contact-form.6">
                <source>formHasBeenSentThankYou</source>
                <target>Wiadomość została wysłana, dziękujemy za kontakt.</target>
            </trans-unit>
            <trans-unit id="tuliacms.contact-form.7">
                <source>formNotHasBeenSentTryAgain</source>
                <target>Wiadomość nie została wysłana, proszę spróbować ponownie. Przepraszamy za niedogodności.</target>
            </trans-unit>
            <trans-unit id="tuliacms.contact-form.8">
                <source>formSaved</source>
                <target>Formularz został zapisany.</target>
            </trans-unit>
            <trans-unit id="tuliacms.contact-form.9">
                <source>formNotFound</source>
                <target>Nie znaleziono takiego formularza.</target>
            </trans-unit>
            <trans-unit id="tuliacms.contact-form.10">
                <source>editForm</source>
                <target>Edycja formularza</target>
            </trans-unit>
            <trans-unit id="tuliacms.contact-form.11">
                <source>messageTemplate</source>
                <target>Szablon wiadomości</target>
            </trans-unit>
