            </trans-unit>
            <trans-unit id="tuliacms.contact-form.2">
                <source>list</source>
                <target>Lista formularzy</target>
            </trans-unit>
            <trans-unit id="tuliacms.contact-form.3">
                <source>form</source>
                <target>Formularz</target>
            </trans-unit>
