                <target>Odbiorcy</target>
            </trans-unit>
            <trans-unit id="tuliacms.contact-form.13">
                <source>receiversHelpText</source>
                <target>Możesz wprowadzić wielu odbiorców, oddzielonych przecinkiem.</target>
            </trans-unit>
            <trans-unit id="tuliacms.contact-form.14">
                <source>messageSubject</source>
                <target>Tytuł wiadomości</target>
            </trans-unit>
            <trans-unit id="tuliacms.contact-form.15">
